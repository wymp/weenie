{
  "name": "@wymp/weenie-framework",
  "version": "0.7.3",
  "description": "This is the main package for Weenie, an unopinionated, fluent, traditional Typescript dependency injector.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npx tsc",
    "docgen": "npx typedoc src/index.ts --sort visibility --sort static-first --sort alphabetical",
    "tsc": "npx tsc",
    "test": "jest",
    "prepublishOnly": "npm run docgen; rm -Rf dist; npx tsc",
    "prepare": "husky install"
  },
  "author": "Kael Shipman<kael.shipman@gmail.com>",
  "license": "ISC",
  "repository": "https://github.com/wymp/weenie-framework.git",
  "dependencies": {
    "@types/cron": "^1.7.3",
    "@types/node": ">=10.17.60",
    "@types/uuid": "^8.3.3",
    "@wymp/http-errors": "^1.8.0 || ^2.0.0",
    "@wymp/http-utils": "^3.0.0",
    "@wymp/simple-db-mysql": "^0.5.2 || ^0.6.0",
    "@wymp/simple-http-client-rpn": "^0.5.2 || ^0.6.0",
    "@wymp/simple-http-server-express": "^0.5.2 || ^0.6.0",
    "@wymp/simple-logger-console": "^0.5.1 || ^0.6.0",
    "@wymp/simple-pubsub-amqp": "^0.5.1 || ^0.6.0",
    "@wymp/ts-simple-interfaces": "^0.5.3 || ^0.6.0",
    "@wymp/weenie-base": "^0.5.4 || ^0.6.0 || ^1.0.0",
    "cron": "^1.8.2",
    "runtypes": "^6.5.0",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.3",
    "@wymp/ts-simple-interfaces-testing": "^0.5.1 || ^0.6.0",
    "husky": "^8.0.0",
    "jest": "^29.0.3",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.0.1",
    "typedoc": "^0.23.15",
    "typescript": "^4.8.3"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "es5"
  },
  "jest": {
    "roots": [
      "<rootDir>/tests"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
